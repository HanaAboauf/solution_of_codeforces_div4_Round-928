#include <bits/stdc++.h>
using namespace std;
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
 
    int t;
    cin>>t;
    while(t--){
        string s;
        int a=0,b=0;
        cin>>s;
        for(char c:s){
            if(c=='A')
                a++;
            else
                b++;
        }
        cout<<((a>b)?'A':'B')<<"\n";
 
    }
}
